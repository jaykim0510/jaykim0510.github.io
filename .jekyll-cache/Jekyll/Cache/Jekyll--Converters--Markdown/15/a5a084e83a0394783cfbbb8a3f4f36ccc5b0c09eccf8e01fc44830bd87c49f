        <span class="n">p</span><span class="o">.</span><span class="na">introduceMyself</span><span class="o">();</span>

        <span class="nc">Worker</span> <span class="n">w</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Worker</span><span class="o">();</span>
        <span class="n">w</span><span class="o">.</span><span class="na">age</span> <span class="o">=</span> <span class="mi">25</span><span class="o">;</span>
        <span class="n">w</span><span class="o">.</span><span class="na">name</span> <span class="o">=</span> <span class="s">"Peter"</span><span class="o">;</span>
