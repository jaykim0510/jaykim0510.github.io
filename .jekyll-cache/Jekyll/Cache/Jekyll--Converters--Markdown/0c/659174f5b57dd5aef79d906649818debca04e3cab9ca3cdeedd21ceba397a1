<span class="k">print</span><span class="p">(</span><span class="n">seq_iterator</span><span class="p">.</span><span class="n">__next__</span><span class="p">())</span> <span class="c1"># ë˜ëŠ” next(seq_iterator)
</span><span class="k">print</span><span class="p">(</span><span class="n">seq_iterator</span><span class="p">.</span><span class="n">__next__</span><span class="p">())</span>
