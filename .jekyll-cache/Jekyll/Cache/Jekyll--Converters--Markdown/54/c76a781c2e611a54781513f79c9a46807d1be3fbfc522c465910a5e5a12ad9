<span class="p">[</span><span class="mi">1</span><span class="p">,</span> <span class="mi">2</span><span class="p">,</span> <span class="mi">3</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="mi">5</span><span class="p">]</span>

<span class="o">&gt;&gt;&gt;</span> <span class="n">a</span> <span class="o">=</span> <span class="p">[</span><span class="s">'ì•ˆë…•'</span><span class="p">,</span> <span class="s">'Hello'</span><span class="p">,</span> <span class="s">'Hi'</span><span class="p">,</span> <span class="s">'ì•ˆë…•í•˜ì‹­ë‹ˆê¹Œ'</span><span class="p">]</span>
