</span>    <span class="k">def</span> <span class="nf">startsWith</span><span class="p">(</span><span class="bp">self</span><span class="p">,</span> <span class="n">prefix</span><span class="p">):</span>
        <span class="n">node</span><span class="o">=</span><span class="bp">self</span><span class="p">.</span><span class="n">root</span>
