    <span class="nl">color</span><span class="p">:</span> <span class="kr">string</span>
    <span class="nx">velocity</span><span class="p">:</span> <span class="kr">number</span>
    <span class="nx">maxVelocity</span><span class="p">:</span> <span class="kr">number</span>
    
    <span class="kd">constructor</span><span class="p">(</span><span class="nx">color</span><span class="p">:</span> <span class="kr">string</span><span class="p">,</span> <span class="nx">velocity</span><span class="p">:</span> <span class="kr">number</span><span class="p">,</span> <span class="nx">maxVelocity</span><span class="p">:</span> <span class="kr">number</span><span class="p">)</span> <span class="p">{</span>
