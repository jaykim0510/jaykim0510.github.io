<span class="o">+</span><span class="c1">-------------------+</span>
</code></pre></div></div>

<div class="language-sql highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="c1">-- REPLACE</span>

<span class="k">SELECT</span> <span class="k">REPLACE</span><span class="p">(</span><span class="nv">"Apple in Apple"</span><span class="p">,</span> <span class="nv">"Apple"</span><span class="p">,</span> <span class="nv">"Samsung"</span><span class="p">);</span>
<span class="c1">------------------------------------------</span>
<span class="o">+</span><span class="c1">-----------------------------------------------+</span>
<span class="o">|</span> <span class="k">REPLACE</span><span class="p">(</span><span class="nv">"Apple in Apple"</span><span class="p">,</span> <span class="nv">"Apple"</span><span class="p">,</span> <span class="nv">"Samsung"</span><span class="p">)</span> <span class="o">|</span>
<span class="o">+</span><span class="c1">-----------------------------------------------+</span>
<span class="o">|</span> <span class="n">Samsung</span> <span class="k">in</span> <span class="n">Samsung</span>                            <span class="o">|</span>
<span class="o">+</span><span class="c1">-----------------------------------------------+</span>
</code></pre></div></div>

<div class="language-sql highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="c1">-- REVERSE</span>

<span class="k">SELECT</span> <span class="n">REVERSE</span><span class="p">(</span><span class="nv">"SQL Tutorial"</span><span class="p">);</span>
<span class="c1">------------------------------------------</span>
<span class="o">+</span><span class="c1">-------------------------+</span>
<span class="o">|</span> <span class="n">REVERSE</span><span class="p">(</span><span class="nv">"SQL Tutorial"</span><span class="p">)</span> <span class="o">|</span>
<span class="o">+</span><span class="c1">-------------------------+</span>
<span class="o">|</span> <span class="n">lairotuT</span> <span class="n">LQS</span>            <span class="o">|</span>
<span class="o">+</span><span class="c1">-------------------------+</span>
</code></pre></div></div>

<div class="language-sql highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="c1">-- RIGHT</span>

<span class="k">SELECT</span> <span class="k">RIGHT</span><span class="p">(</span><span class="nv">"SQL Tutorial is cool"</span><span class="p">,</span> <span class="mi">4</span><span class="p">)</span> <span class="k">AS</span> <span class="n">ExtractString</span><span class="p">;</span>
<span class="c1">------------------------------------------</span>
<span class="o">+</span><span class="c1">---------------+</span>
<span class="o">|</span> <span class="n">ExtractString</span> <span class="o">|</span>
<span class="o">+</span><span class="c1">---------------+</span>
<span class="o">|</span> <span class="n">cool</span>          <span class="o">|</span>
<span class="o">+</span><span class="c1">---------------+</span>
</code></pre></div></div>

<div class="language-sql highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="c1">-- RPAD</span>

<span class="k">SELECT</span> <span class="n">RPAD</span><span class="p">(</span><span class="nv">"SQL Tutorial"</span><span class="p">,</span> <span class="mi">20</span><span class="p">,</span> <span class="nv">"+++"</span><span class="p">);</span>
<span class="c1">------------------------------------------</span>
<span class="o">+</span><span class="c1">---------------------------------+</span>
<span class="o">|</span> <span class="n">RPAD</span><span class="p">(</span><span class="nv">"SQL Tutorial"</span><span class="p">,</span> <span class="mi">20</span><span class="p">,</span> <span class="nv">"+++"</span><span class="p">)</span> <span class="o">|</span>
<span class="o">+</span><span class="c1">---------------------------------+</span>
<span class="o">|</span> <span class="k">SQL</span> <span class="n">Tutorial</span><span class="o">++++++++</span>            <span class="o">|</span>
<span class="o">+</span><span class="c1">---------------------------------+</span>
</code></pre></div></div>

<div class="language-sql highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="c1">-- RTRIM</span>

<span class="k">SELECT</span> <span class="n">RTRIM</span><span class="p">(</span><span class="nv">"SQL Tutorial     "</span><span class="p">)</span> <span class="k">AS</span> <span class="n">RightTrimmedString</span><span class="p">;</span>
<span class="c1">------------------------------------------</span>
<span class="o">+</span><span class="c1">--------------------+</span>
<span class="o">|</span> <span class="n">RightTrimmedString</span> <span class="o">|</span>
<span class="o">+</span><span class="c1">--------------------+</span>
<span class="o">|</span> <span class="k">SQL</span> <span class="n">Tutorial</span>       <span class="o">|</span>
<span class="o">+</span><span class="c1">--------------------+</span>
</code></pre></div></div>

