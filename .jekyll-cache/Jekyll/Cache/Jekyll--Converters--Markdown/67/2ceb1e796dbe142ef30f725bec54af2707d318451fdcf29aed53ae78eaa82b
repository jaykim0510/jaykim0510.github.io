<span class="k">DISTINCT</span><span class="p">(</span><span class="n">gender</span><span class="p">)</span>

<span class="c1">-- ë¬¸ìì—´ ë°ì´í„° íƒ€ì…</span>
<span class="k">SUBSTRING</span><span class="p">(</span><span class="n">address</span><span class="p">,</span> <span class="mi">1</span><span class="p">,</span> <span class="mi">2</span><span class="p">)</span> <span class="c1">-- addressì˜ ì²«ë²ˆì§¸ ë¬¸ìì—ì„œ 2ê°œ</span>
<span class="k">LENGTH</span><span class="p">(</span><span class="n">address</span><span class="p">)</span>
