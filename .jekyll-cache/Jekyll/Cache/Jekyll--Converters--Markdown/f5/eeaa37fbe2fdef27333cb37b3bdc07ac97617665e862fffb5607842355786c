<span class="n">feature_vector</span> <span class="o">=</span> <span class="n">store</span><span class="p">.</span><span class="n">get_online_features</span><span class="p">(</span>
    <span class="n">feature_refs</span><span class="o">=</span><span class="p">[</span><span class="s">"project_details:text"</span><span class="p">,</span> <span class="s">"project_details:tag"</span><span class="p">],</span>
    <span class="n">entity_rows</span><span class="o">=</span><span class="p">[{</span><span class="s">"id"</span><span class="p">:</span> <span class="mi">6</span><span class="p">}],</span>
<span class="p">).</span><span class="n">to_dict</span><span class="p">()</span>

