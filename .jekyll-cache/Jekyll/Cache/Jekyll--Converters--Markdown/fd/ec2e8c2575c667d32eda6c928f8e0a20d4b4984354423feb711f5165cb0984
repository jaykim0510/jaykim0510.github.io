    <span class="k">return</span> <span class="nx">el</span><span class="p">.</span><span class="nx">length</span> <span class="o">===</span> <span class="mi">4</span> <span class="p">});</span>


<span class="kd">const</span> <span class="nx">everyReturn</span> <span class="o">=</span> <span class="nx">names</span><span class="p">.</span><span class="nx">every</span><span class="p">((</span><span class="nx">el</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">{</span>
