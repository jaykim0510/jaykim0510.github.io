    <span class="na">name</span><span class="p">:</span> <span class="dl">'</span><span class="s1">Captain</span><span class="dl">'</span><span class="p">,</span>
<span class="p">}</span>

<span class="kd">const</span> <span class="nx">user_with_cat</span> <span class="o">=</span> <span class="p">{</span>
    <span class="na">name</span><span class="p">:</span> <span class="dl">'</span><span class="s1">Captain</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">cat</span><span class="p">:</span> <span class="p">{</span>
      <span class="na">breed</span><span class="p">:</span> <span class="dl">'</span><span class="s1">British Shorthair</span><span class="dl">'</span><span class="p">,</span>
    <span class="p">}</span>
<span class="p">}</span>

<span class="nx">printCatName1</span><span class="p">(</span><span class="nx">user</span><span class="p">)</span> <span class="c1">// TypeError</span>
