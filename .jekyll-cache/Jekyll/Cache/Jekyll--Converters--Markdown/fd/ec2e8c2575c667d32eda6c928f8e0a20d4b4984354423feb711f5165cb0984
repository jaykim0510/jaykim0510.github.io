<h1 id="reduce">reduce</h1>

<div class="language-js highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kd">const</span> <span class="nx">numbers</span> <span class="o">=</span> <span class="p">[</span><span class="mi">1</span><span class="p">,</span> <span class="mi">2</span><span class="p">,</span> <span class="mi">3</span><span class="p">,</span> <span class="mi">4</span><span class="p">];</span>

<span class="kd">const</span> <span class="nx">totalSum</span> <span class="o">=</span> <span class="nx">numbers</span><span class="p">.</span><span class="nx">reduce</span><span class="p">((</span><span class="nx">acc</span><span class="p">,</span> <span class="nx">el</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">{</span>
    <span class="k">return</span> <span class="nx">acc</span> <span class="o">+</span> <span class="nx">el</span><span class="p">;</span>
<span class="p">},</span> <span class="mi">0</span><span class="p">)</span>

<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">totalSum</span><span class="p">);</span>
</code></pre></div></div>

<h1 id="sort-reverse">sort, reverse</h1>

<div class="language-js highlighter-rouge"><div class="highlight"><pre class="highlight"><code>
<span class="kd">const</span> <span class="nx">numbers</span> <span class="o">=</span> <span class="p">[</span><span class="mi">1</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="mi">7</span><span class="p">,</span> <span class="mi">10</span><span class="p">];</span>

<span class="c1">// sort,reverse ëŠ” ì›ë³¸ ë°°ì—´ numbersë¥¼ ì •ë ¬í•¨ -&gt; ì›ë³¸ì„ ë³´ì¡´í•˜ë ¤ë©´ ìƒˆë¡œìš´ ë³€ìˆ˜ì— ë¯¸ë¦¬ í• ë‹¹í•´ë‘¬ì•¼í•¨</span>

<span class="c1">// ìœ ë‹ˆì½”ë“œì— ì •ì˜ëœ ìˆœìœ¼ë¡œ ì •ë ¬</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">numbers</span><span class="p">.</span><span class="nx">sort</span><span class="p">());</span> <span class="c1">// [1, 10, 4, 7]</span>

<span class="c1">// ì˜¤ë¦„ì°¨ìˆœ ì •ë ¬</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">numbers</span><span class="p">.</span><span class="nx">sort</span><span class="p">((</span><span class="nx">a</span><span class="p">,</span> <span class="nx">b</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="nx">a</span> <span class="o">-</span> <span class="nx">b</span><span class="p">));</span> <span class="c1">// [1, 4, 7, 10]</span>

<span class="c1">// ë‚´ë¦¼ì°¨ìˆœ ì •ë ¬</span>
<span class="nx">numbers</span><span class="p">.</span><span class="nx">sort</span><span class="p">((</span><span class="nx">a</span><span class="p">,</span> <span class="nx">b</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="nx">b</span> <span class="o">-</span> <span class="nx">a</span><span class="p">);</span> <span class="c1">// [10, 7, 4, 1]</span>

<span class="c1">// ì—­ìœ¼ë¡œ ì •ë ¬</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">numbers</span><span class="p">.</span><span class="nx">reverse</span><span class="p">());</span> <span class="c1">// [10, 7, 4, 1]</span>
</code></pre></div></div>

<h1 id="mapê³¼-set">Mapê³¼ Set</h1>

<div class="language-js highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="c1">// Map ê°ì²´ëŠ” ë©”ì†Œë“œë¥¼ í†µí•´ ê°’ì„ ë‹¤ë£¨ê¸° ë•Œë¬¸ì—, ë‹¤ì–‘í•œ ìë£Œí˜•ì„ keyë¡œ í™œìš©í•  ìˆ˜ ìˆë‹¤ëŠ” ì¥ì </span>

<span class="c1">// Map ìƒì„±</span>
<span class="kd">const</span> <span class="nx">myMap</span> <span class="o">=</span> <span class="k">new</span> <span class="nb">Map</span><span class="p">();</span>

<span class="c1">// set ë©”ì†Œë“œ</span>
<span class="nx">myMap</span><span class="p">.</span><span class="kd">set</span><span class="p">(</span><span class="dl">'</span><span class="s1">title</span><span class="dl">'</span><span class="p">,</span> <span class="dl">'</span><span class="s1">ë¬¸ìì—´ key</span><span class="dl">'</span><span class="p">);</span>
<span class="nx">myMap</span><span class="p">.</span><span class="kd">set</span><span class="p">(</span><span class="mi">2017</span><span class="p">,</span> <span class="dl">'</span><span class="s1">ìˆ«ìí˜• key</span><span class="dl">'</span><span class="p">);</span>
<span class="nx">myMap</span><span class="p">.</span><span class="kd">set</span><span class="p">(</span><span class="kc">true</span><span class="p">,</span> <span class="dl">'</span><span class="s1">ë¶ˆë¦°í˜• key</span><span class="dl">'</span><span class="p">);</span>

<span class="c1">// get ë©”ì†Œë“œ</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="kd">get</span><span class="p">(</span><span class="mi">2017</span><span class="p">));</span> <span class="c1">// ìˆ«ìí˜• key</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="kd">get</span><span class="p">(</span><span class="kc">true</span><span class="p">));</span> <span class="c1">// ë¶ˆë¦°í˜• key</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="kd">get</span><span class="p">(</span><span class="dl">'</span><span class="s1">title</span><span class="dl">'</span><span class="p">));</span> <span class="c1">// ë¬¸ìì—´ key</span>

<span class="c1">// has ë©”ì†Œë“œ</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="nx">has</span><span class="p">(</span><span class="dl">'</span><span class="s1">title</span><span class="dl">'</span><span class="p">));</span> <span class="c1">// true</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="nx">has</span><span class="p">(</span><span class="dl">'</span><span class="s1">name</span><span class="dl">'</span><span class="p">));</span> <span class="c1">// false</span>

<span class="c1">// size í”„ë¡œí¼í‹°</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="nx">size</span><span class="p">);</span> <span class="c1">// 3</span>

<span class="c1">// delete ë©”ì†Œë“œ</span>
<span class="nx">myMap</span><span class="p">.</span><span class="k">delete</span><span class="p">(</span><span class="kc">true</span><span class="p">);</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="kd">get</span><span class="p">(</span><span class="kc">true</span><span class="p">));</span> <span class="c1">// undefined</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="nx">size</span><span class="p">);</span> <span class="c1">// 2</span>

<span class="c1">// clear ë©”ì†Œë“œ</span>
<span class="nx">myMap</span><span class="p">.</span><span class="nx">clear</span><span class="p">();</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="kd">get</span><span class="p">(</span><span class="mi">2017</span><span class="p">));</span> <span class="c1">// undefined</span>
<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myMap</span><span class="p">.</span><span class="nx">size</span><span class="p">);</span> <span class="c1">// 0</span>
</code></pre></div></div>

